$("#user-materials-list").html("<%= j(render 'public/materials/index', materials: @materials) %>");

// タブの active クラス切り替え
$(".nav-tabs .nav-link").removeClass("active"); // 全部外す
$(".nav-tabs .nav-link[href$='status=<%= params[:status] %>']").addClass("active");

// 「すべて」タブ用に status パラメータがない場合
<% if params[:status].blank? %>
  $(".nav-tabs .nav-link[href='<%= users_mypage_path %>']").addClass("active");
<% end %>